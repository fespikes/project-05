{
  "name": "warpstor-frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "test:single": "ng t warpstor-frontend -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint warpstor-frontend --type-check",
    "lint:fix": "ng lint tcc --fix --type-check",
    "e2e": "ng e2e",
    "hmr": "ng serve --configuration hmr --proxy-config proxy.conf.json",
    "prestart": "npm run i18n",
    "prebuild": "npm run i18n",
    "prebuild:prod": "npm run i18n",
    "build:prod": "ng build --prod --aot",
    "lite": "./node_modules/lite-server/bin/lite-server -c ./bs-config.js",
    "cas": "node server/cas",
    "i18n": "node scripts/compileI18n",
    "icon": "node scripts/generateIconSymbol",
    "analyze": "ng build --aot --prod --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/cdk": "^7.3.2",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "cheerio": "^1.0.0-rc.2",
    "core-js": "^2.5.4",
    "lodash-es": "^4.17.11",
    "quill": "^1.3.6",
    "rxjs": "~6.3.3",
    "tdc-ui": "^0.7.14",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.2",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "^7.2.5",
    "@angular/language-service": "~7.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
